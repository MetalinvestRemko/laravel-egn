{
    "name": "metalinvestremko/laravel-egn",
    "description": "Laravel package for validating and generating Bulgarian EGN (ЕГН).",
    "homepage": "https://egn.bg/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MetalinvestRemko"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MetalinvestRemko\\LaravelEgn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetalinvestRemko\\LaravelEgn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MetalinvestRemko\\LaravelEgn\\EgnServiceProvider"
            ],
            "aliases": {
                "Egn": "MetalinvestRemko\\LaravelEgn\\Facades\\Egn"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
